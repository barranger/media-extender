 {   
 	"name": "Media Expander",   
 	"version": "1.0",   
  	"manifest_version": 2,
 	"description": "Expands media for http://alpha.app.net",
 	"content_scripts": [
        {
            "matches": [ "http://alpha.app.net/*", "https://alpha.app.net/*" ],
            "js":      [ "lib/jquery-1.8.0.min.js", "processors.js", "media-expander.js" ]
        }
    ],
    "permissions": [
        "http://alpha.app.net/*",
        "https://alpha.app.net/*",
        "https://api.twitter.com/*"
    ]
 } 